{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/href-no-hash": 0,
    "padded-blocks": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-return-assign": 0,
    "no-use-before-define": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0
  }
}
